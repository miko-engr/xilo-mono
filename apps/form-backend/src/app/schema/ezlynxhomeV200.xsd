  </xsd:simpleType>

  <xsd:complexType name="EarthquakeTypeType">
    <xsd:sequence>
      <xsd:element name="Zone" type="EarthquakeZoneType" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="ExcludeMasonryVeneer" type="BooleanType" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="Deductible" type="EarthquakeDeductibleType" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="PercentVeneer" type="xsd:nonNegativeInteger" minOccurs="0" maxOccurs="1"/>
