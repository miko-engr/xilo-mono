      <xsd:enumeration value="17" />
      <xsd:enumeration value="18" />
      <xsd:enumeration value="19" />
      <xsd:enumeration value="20" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="DateType">
    <xsd:restriction base="xsd:date" />
  </xsd:simpleType>
  <xsd:simpleType name="BooleanType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Yes" />
      <xsd:enumeration value="No" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MaritalStatusType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Single"/>
      <xsd:enumeration value="Married"/>
      <xsd:enumeration value="Domestic Partner"/>
      <xsd:enumeration value="Widowed"/>
      <xsd:enumeration value="Separated"/>
      <xsd:enumeration value="Divorced"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OccupationType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Self-Employed"/>
      <xsd:enumeration value="Employed"/>
      <xsd:enumeration value="Unemployed"/>
      <xsd:enumeration value="Homemaker"/>
      <xsd:enumeration value="Retired"/>
      <xsd:enumeration value="Student"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="GenderType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Male" />
      <xsd:enumeration value="Female" />
      <xsd:enumeration value="X - Not Specified" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="RelationType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Child"/>
      <xsd:enumeration value="Domestic Partner"/>
      <xsd:enumeration value="Employee"/>
      <xsd:enumeration value="Insured"/>
      <xsd:enumeration value="Other"/>
      <xsd:enumeration value="Parent"/>
      <xsd:enumeration value="Relative"/>
      <xsd:enumeration value="Spouse"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="EducationType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="No High School Diploma" />
      <xsd:enumeration value="High School Diploma" />
      <xsd:enumeration value="Some College - No Degree" />
      <xsd:enumeration value="Vocational/Technical Degree" />
      <xsd:enumeration value="Associates Degree" />
      <xsd:enumeration value="Bachelors" />
      <xsd:enumeration value="Masters" />
      <xsd:enumeration value="Phd" />
      <xsd:enumeration value="Medical Degree" />
      <xsd:enumeration value="Law Degree" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PersonalInfoType">
    <xsd:sequence>
      <xsd:element name="Name" type="NameType" />
      <xsd:element name="DOB" type="DateType" />
      <xsd:element name="SSN" type="SSNType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Gender" type="GenderType"/>
      <xsd:element name="MaritalStatus" type="MaritalStatusType"/>
      <xsd:element name="Relation" type="RelationType"/>
      <xsd:element name="Industry" type="xsd:string" />
      <xsd:element name="Occupation" type="xsd:string" />
      <xsd:element name="YearsWithEmployer" type="xsd:nonNegativeInteger" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="YearsWithPreviousEmployer" type="xsd:nonNegativeInteger" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Education" type="EducationType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="MaidenName" type="xsd:string" minOccurs="0" maxOccurs="1" />
      <xsd:element name="NickName" type="xsd:string" minOccurs="0" maxOccurs="1" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="AddressCodeType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BillingAddress" />
      <xsd:enumeration value="InsuredsAddress" />
      <xsd:enumeration value="LegalAddr" />
      <xsd:enumeration value="MailingAddress" />
      <xsd:enumeration value="PhysicalRisk" />
      <xsd:enumeration value="PreviousAddress" />
      <xsd:enumeration value="StreetAddress" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="C-10">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="10" />
      <xsd:minLength value="0" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="C-10_NoBlank">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="10" />
      <xsd:minLength value="1" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="C-50">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="50" />
      <xsd:minLength value="0" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="C-50_NoBlank">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="50" />
      <xsd:minLength value="1" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="C-255">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255" />
      <xsd:minLength value="0" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="C-255_NoBlank">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="255" />
      <xsd:minLength value="1" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ProperName">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="^[A-Za-z]{1}[\-' A-Za-z]*$" />
      <xsd:maxLength value="64" />
      <xsd:minLength value="1" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="MiddleInitial">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="^[A-Za-z]?$" />
    </xsd:restriction>
  </xsd:simpleType>

