      <xsd:element name="Furs" type="ScheduledPersonalPropertyTypeType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="FineArts" type="ScheduledPersonalPropertyTypeType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="Guns" type="ScheduledPersonalPropertyTypeType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="Jewelry" type="ScheduledPersonalPropertyTypeType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="Music" type="ScheduledPersonalPropertyTypeType" minOccurs="0" maxOccurs="1" />
