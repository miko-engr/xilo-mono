  <xsd:complexType name="ApplicantType">
    <xsd:sequence>
      <xsd:element name="ApplicantType" type="ApplicantTypeType" />
      <xsd:element name="PersonalInfo" type="PersonalInfoType" />
      <xsd:element name="Address" type="AddressType" minOccurs="0" maxOccurs="unbounded" />
    </xsd:sequence>
  </xsd:complexType>

