    <xsd:sequence>
      <xsd:element name="DateOfIncident" type="DateType" />
      <xsd:element name="Description" type="LossDescriptionType" />
      <xsd:element name="Amount" type="xsd:decimal" />
      <xsd:element name="CATLoss" type="BooleanType" />
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:positiveInteger" />
