        <xsd:element name="PolicyInfo" type="PolicyInfoType" minOccurs="1" maxOccurs="1" />
        <xsd:element name="RatingInfo" type="RatingInfoType" minOccurs="1" maxOccurs="1" />
        <xsd:element name="ReplacementCost" type="ReplacementCostType" minOccurs="1" maxOccurs="1" />
        <xsd:element name="Endorsements" type="EndorsementsType" minOccurs="1" maxOccurs="1" />
        <xsd:element name="LossInfo" type="LossInfoType" minOccurs="0" maxOccurs="unbounded" />
        <xsd:element name="GeneralInfo" type="GeneralInfoType" minOccurs="0" maxOccurs="1" />
        <xsd:element name="ExtendedInfo" type="ExtendedInfoType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>