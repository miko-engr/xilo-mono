  </xsd:complexType>
  <!-- End of PA Endorsements -->

  <!-- TX Specific Endorsements -->
  <xsd:complexType name="TX-StateSpecific">
    <xsd:sequence>
      <xsd:element name="ResidenceGlass" type="BooleanType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="WindHailExclusion" type="BooleanType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="FoundationCoverage" type="TX-FoundationCoverageType" minOccurs="0" maxOccurs="1"/>
