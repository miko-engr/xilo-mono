      <xsd:enumeration value="Animal Boarding/Grooming/Breeding/Riding/Veterinarian" />
      <xsd:enumeration value="Attorney" />
      <xsd:enumeration value="Auto/Vehicle Repair" />
      <xsd:enumeration value="Clerical" />
      <xsd:enumeration value="Consultant" />
	  <xsd:enumeration value="Contractor/Artisan/Landscaper" />
	  <xsd:enumeration value="Electronic/Machine Repair" />
	  <xsd:enumeration value="Farming/Ranching" />
