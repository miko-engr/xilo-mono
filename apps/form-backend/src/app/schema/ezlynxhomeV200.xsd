  <xsd:complexType name="NC-StateSpecific">
    <xsd:sequence>
      <xsd:element name="WindHailExclusion" type="BooleanType" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
