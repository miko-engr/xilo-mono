      <xsd:element name="CreditCardLimit" type="CreditCardLimitType" minOccurs="0" maxOccurs="1" />
      <!-- label should read 'Increased Coverage on Credit Card' -->
      <xsd:element name="Earthquake" type="EarthquakeType" minOccurs="0" maxOccurs="1"  />
      <xsd:element name="IdentityTheft" type="IdentityTheftType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="IncreasedMold" type="IncreasedMoldType"  minOccurs="0" maxOccurs="1" />
      <xsd:element name="LossAssessment" type="LossAssessmentType"  minOccurs="0" maxOccurs="1" />
      <xsd:element name="OrdinanceOrLaw" type="OrdinanceOrLawType"  minOccurs="0" maxOccurs="1" />
      <xsd:element name="ProtectiveDevices" type="ProtectiveDevicesType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="PersonalInjury" type="PersonalInjuryType"  minOccurs="0" maxOccurs="1" />
      <xsd:element name="ScheduledPersonalProperty" type="ScheduledPersonalPropertyType"  minOccurs="0" maxOccurs="1" />
      <xsd:element name="ReplacementCostDwelling" type="ReplacementCostDwellingType" minOccurs="0" maxOccurs="1"  />
      <xsd:element name="ReplacementCostContent" type="ReplacementCostContentType" minOccurs="0" maxOccurs="1"  />
      <xsd:element name="IncreasedJewelryWatchesFurs" type="JewelryWatchesFursType" minOccurs="0" maxOccurs="1"  />
      <!-- label should read 'Increased Limit On Jewelry, Watches, & Furs'  -->
      <xsd:element name="WaterBackup" type="WaterBackupType"  minOccurs="0" maxOccurs="1" />
      <xsd:element name="StateSpecificEndorsements" type="StateSpecificEndorsementsType" minOccurs="0" maxOccurs="1" />
    </xsd:sequence>
